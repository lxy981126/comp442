main
{
    var{
        integer x;
    }
    x = 1 + 3;
    write(x);

    x = 6 - 4;
    write(x);

    x = 3 * 5;
    write(x);

    x = 8 / 2;
    write(x);

    if (x > 7) then {
        write(8);
    }
    else{
        write(0);
    };

    x = 0;
    while (x <= 10) {
        write(x);
        x = x + 1;
    };
}